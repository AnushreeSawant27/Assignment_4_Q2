   "source": [
    "# Write a Python program to triple all numbers of a given list of integers. Use Python map. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "c1ed5162",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Triple of list numbers: [3, 6, 9, 12, 15, 18, 21]\n"
     ]
    }
   ],
   "source": [
    "def triple_number(x):\n",
    "    return x * 3\n",
    "\n",
    "numbers = [1, 2, 3, 4, 5, 6, 7]\n",
    "tripled_numbers = list(map(triple_number, numbers))\n",
    "\n",
    "print('Triple of list numbers:',tripled_numbers)"
   ]
  }
